{
  "name": "bati",
  "private": true,
  "version": "0.0.54",
  "description": "@batijs monorepo",
  "scripts": {
    "release": "pnpm -r prerelease && bumpp && pnpm run publish",
    "release:ci": "pnpm -r prerelease && bumpp -y && pnpm run publish",
    "publish": "pnpm -r build && pnpm -r publish",
    "watch": "pnpm -r --parallel build --watch",
    "build": "pnpm -r build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=v16.20.0",
    "pnpm": ">=8.6.3"
  },
  "pnpm": {
    "overrides": {
      "recast": "npm:@putout/recast",
      "esbuild": "^0.18.16"
    }
  }
}
