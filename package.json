{
  "name": "bati",
  "private": true,
  "version": "0.0.23",
  "description": "@batijs monorepo",
  "scripts": {
    "release": "pnpm -r prerelease && bumpp -r --commit --push --tag && pnpm run publish",
    "publish": "pnpm -r build && pnpm -r publish",
    "watch": "pnpm -r --parallel build --watch",
    "build": "pnpm -r build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=v16.20.0",
    "pnpm": ">=8.6.2"
  },
  "pnpm": {
    "overrides": {
      "recast": "npm:@putout/recast@1.12.1"
    }
  }
}
