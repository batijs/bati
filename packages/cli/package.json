{
  "name": "@batijs/cli",
  "version": "0.0.23",
  "description": "Scaffold typescript projects with ease and modularity",
  "type": "module",
  "scripts": {
    "prerelease": "rm -rf ./dist",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm run build && vitest run --test-timeout=50000",
    "build": "rm -rf dist && tsup"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@batijs/build": "workspace:*",
    "@batijs/tsup": "workspace:*",
    "tsup": "^6.7.0",
    "@types/node": "^18.16.17",
    "citty": "^0.1.1",
    "colorette": "^2.0.20",
    "esbuild": "^0.17.19",
    "espree": "^9.5.2",
    "execa": "^7.1.1",
    "get-port": "^7.0.0",
    "node-fetch": "^3.3.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0",
    "wait-for-localhost": "^4.0.1"
  },
  "dependencies": {
    "@batijs/core": "workspace:*"
  },
  "bin": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/"
  ]
}
